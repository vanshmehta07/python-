# Program to find sum of diagonal elements of a 3x3 matrix

matrix = [1, 2, 3, 4, 5, 6, 7, 8, 9]

n = 3
diagonal_sum = 0
diagonal_elements = []

for i in range(n):
    diagonal_elements.append(matrix[i * n + i])
    diagonal_sum += matrix[i * n + i]

print("Diagonal elements are:", ",".join(map(str, diagonal_elements)))
print("Sum of diagonal elements =", diagonal_sum)
